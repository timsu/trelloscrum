{
   "background": {
      "scripts": [ "showPageAction.js" ]
   },
   "content_scripts": [ {
      "css": [ "trelloscrum.css" ],
      "js": [ "jquery-1.7.1.min.js", "mutation_summary.js", "trelloscrum.js" ],
      "matches": [ "https://trello.com/*" ]
   } ],
   "description": "",
   "icons": {
      "128": "images/ogd-trello128.png"
   },
   "manifest_version": 2,
   "name": "Trello Separator + Labels",
   "page_action": {
      "default_icon": {
         "19": "images/ogd-trello19.png",
         "38": "images/ogd-trello38.png"
      },
      "default_popup": "popup.html",
      "default_title": "Board settings"
   },
   "permissions": [ "tabs" ],
   "version": "8"
}
